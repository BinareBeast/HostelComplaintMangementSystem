<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".about">
  <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="119dp"
      android:orientation="vertical">

  </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="355dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/nm1"
            android:layout_width="match_parent"
            android:layout_height="31dp"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="22dp"
            android:autoSizeTextType="uniform"
            android:drawableStart="@drawable/baseline_edit_note_24"
            android:gravity="center_vertical"/>
<!--            android:text="Name : " />-->
        <TextView
            android:id="@+id/em1"
            android:layout_width="match_parent"
            android:layout_height="31dp"
            android:layout_marginLeft="25dp"
            android:drawableStart="@drawable/outline_mail_24"
            android:layout_marginTop="15dp"
            android:autoSizeTextType="uniform"
            android:gravity="center_vertical"/>
        <TextView
            android:id="@+id/rn1"
            android:layout_width="match_parent"
            android:layout_height="31dp"
            android:layout_marginLeft="25dp"
            android:drawableStart="@drawable/baseline_edit_note_24"
            android:layout_marginTop="15dp"
            android:autoSizeTextType="uniform"
            android:gravity="center_vertical"/>
        <TextView
            android:id="@+id/ph1"
            android:layout_width="match_parent"
            android:layout_height="31dp"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="15dp"
            android:drawableStart="@drawable/outline_contact_phone_24"
            android:autoSizeTextType="uniform"
            android:gravity="center_vertical"/>


        <TextView
            android:id="@+id/hs1"
            android:layout_width="match_parent"
            android:layout_height="31dp"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="15dp"
            android:autoSizeTextType="uniform"
            android:drawableStart="@drawable/outline_home_work_24"
            android:gravity="center_vertical"/>


    </LinearLayout>


        <com.google.android.material.navigation.NavigationView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:menu="@menu/abtm">

        </com.google.android.material.navigation.NavigationView>


</LinearLayout>